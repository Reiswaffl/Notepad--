<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<?import javafx.scene.image.ImageView?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="main.java.com.Notepad.View.MainSceneController"
            prefHeight="800.0" prefWidth="500.0">
    <children>
        <VBox alignment="TOP_CENTER" spacing="20.0" AnchorPane.topAnchor="20.0" AnchorPane.leftAnchor="20.0" >
            <children>
                <HBox alignment="CENTER" spacing="150.0">
                    <children>
                        <Label  fx:id="userName" text="userName"/>
                        <Button mnemonicParsing="false" onAction="#changeUser" text="CHANGE USER"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER" spacing="20.0">
                    <children>
                        <ScrollPane fx:id="scrollPane" prefWidth="450" prefHeight="550" >
                            <HBox alignment="CENTER" spacing="50.0">
                                <children>
                                    <Label fx:id="content" text="-->"></Label>
                                    <Label fx:id="content" text="My Content"></Label>
                                    <CheckBox></CheckBox>
                                </children>
                            </HBox>
                        </ScrollPane>
                    </children>
                </HBox>
                <HBox spacing="20.0" alignment="CENTER">
                    <children>
                        <MenuButton fx:id="day" text="DAY">
                            <items>
                            <MenuItem  mnemonicParsing="false" onAction="#handleI1" text="01"></MenuItem>
                            <MenuItem  mnemonicParsing="false" onAction="#handleI2" text="02"></MenuItem>
                            <MenuItem  mnemonicParsing="false" onAction="#handleI3" text="03"></MenuItem>
                            <MenuItem  mnemonicParsing="false" onAction="#handleI4" text="04"></MenuItem>
                            <MenuItem  mnemonicParsing="false" onAction="#handleI5" text="05"></MenuItem>
                            <MenuItem  mnemonicParsing="false" onAction="#handleI6" text="06"></MenuItem>
                            <MenuItem  mnemonicParsing="false" onAction="#handleI7" text="07"></MenuItem>
                            <MenuItem  mnemonicParsing="false" onAction="#handleI8" text="08"></MenuItem>
                            <MenuItem  mnemonicParsing="false" onAction="#handleI9" text="09"></MenuItem>
                            <MenuItem  mnemonicParsing="false" onAction="#handleI10" text="10"></MenuItem>
                            <MenuItem  mnemonicParsing="false" onAction="#handleI11" text="11"></MenuItem>
                            <MenuItem  mnemonicParsing="false" onAction="#handleI12" text="12"></MenuItem>
                            <MenuItem  mnemonicParsing="false" onAction="#handleI13" text="13"></MenuItem>
                            <MenuItem  mnemonicParsing="false" onAction="#handleI14" text="14"></MenuItem>
                            <MenuItem  mnemonicParsing="false" onAction="#handleI15" text="15"></MenuItem>
                            <MenuItem  mnemonicParsing="false" onAction="#handleI16" text="16"></MenuItem>
                            <MenuItem  mnemonicParsing="false" onAction="#handleI17" text="17"></MenuItem>
                            <MenuItem  mnemonicParsing="false" onAction="#handleI18" text="18"></MenuItem>
                            <MenuItem  mnemonicParsing="false" onAction="#handleI19" text="19"></MenuItem>
